
<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => true, :autofocus => true %>
  <%= f.label :nationality %>
  <%= f.text_field :nationality %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :image %>
  <%= f.url_field :genre %>

  <%= f.submit %>
<% end %>
