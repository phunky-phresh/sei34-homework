<div class="content">
  <h2>Profiles</h2>


  <table>
    <tr>
      <th>Snowboard Profile</th>
      <th>Rider Level</th>
      <th>Snowboard Shape</th>
    </tr>
    <% @profiles.each do |profile| %>
    <tr>
      <td>
        <a href="/profiles/<%= profile["id"] %>">
        <%= profile["name"] %>
      </td>

    </tr>
    <% end %>

  </table>




</div>
